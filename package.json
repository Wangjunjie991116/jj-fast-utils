{
  "name": "jj-fast-utils",
  "version": "0.0.13",
  "private": false,
  "description": "ðŸš€ FE utils for anyone !",
  "keywords": [
    "FE",
    "Utiles",
    "Anyone",
    "Fast"
  ],
  "homepage": "https://github.com/Wangjunjie991116/jj-fast-utils#readme",
  "bugs": {
    "url": "https://github.com/Wangjunjie991116/jj-fast-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wangjunjie991116/jj-fast-utils.git"
  },
  "license": "MIT",
  "author": "junjie.Wang",
  "type": "commonjs",
  "types": "src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && cross-env NODE_ENV=production && rollup -c --bundleConfigAsCjs",
    "dev": "rm -rf dist && cross-env NODE_ENV=development && rollup -c --bundleConfigAsCjs",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "prepare": "husky install",
    "publish": "sh npm-publish.sh",
    "serve": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*./src/**/*.{js,ts,json,css,less,scss,md,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/qs": "^6.9.8",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-regexp": "1.15.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "prettier": "2.7.1",
    "rollup": "^4.14.3",
    "sort-package-json": "^2.10.0",
    "tslib": "^2.6.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.2.2"
  }
}